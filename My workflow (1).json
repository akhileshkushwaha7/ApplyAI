    {
      "parameters": {
        "mode": "raw",
        "jsonOutput": "={{ (() => {\n  const raw = String(($json.output ?? ''));\n  let s = raw\n    .replace(/`{2,3}(?:json)?\\s*/gi, '')\n    .replace(/\\s*`{3}\\s*$/gi, '');\n  s = s.replace(/[\\u201C\\u201D]/g, '\\\"').replace(/\\u2019/g, \"'\");\n  const endIdx = s.indexOf('END_OF_JSON');\n  if (endIdx !== -1) s = s.slice(0, endIdx);\n  const start = s.indexOf('{');\n  if (start < 0) return {};\n  let depth = 0, inStr = false, esc = false, end = -1;\n  for (let i = start; i < s.length; i++) {\n    const ch = s[i];\n    if (inStr) {\n      if (esc) { esc = false; }\n      else if (ch === '\\\\') { esc = true; }\n      else if (ch === '\\\"') { inStr = false; }\n    } else {\n      if (ch === '\\\"') inStr = true;\n      else if (ch === '{') depth++;\n      else if (ch === '}') {\n        depth--;\n        if (depth === 0) { end = i + 1; break; }\n      }\n    }\n  }\n  if (end < 0) return {};\n  const cleaned = s.slice(start, end).trim();\n  try {\n    return JSON.parse(cleaned);\n  } catch (e) {\n    return {};\n  }\n})() }}\n",
        "options": {}
